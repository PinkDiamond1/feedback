{
  "name": "feedback",
  "version": "0.0.1",
  "author": "Adem ilter <adem@upstash.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "tsup": {
    "entry": [
      "./src/index.tsx"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "splitting": false,
    "sourcemap": false,
    "clean": true
  },
  "dependencies": {
    "@stitches/react": "^1.2.6",
    "@upstash/redis": "^0.2.1",
    "nanoid": "^3.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "prettier": "^2.5.1",
    "tsup": "^5.11.11"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "fluid": false
  },
  "homepage": "https://github.com/upstash/feedback#readme",
  "repository": {
    "type": "git",
    "url": "git+https://ademilter@github.com/upstash/feedback.git"
  },
  "bugs": {
    "url": "https://github.com/upstash/feedback/issues"
  }
}
