{
  "name": "@upstash/feedback",
  "version": "0.0.32",
  "author": "Adem ilter <adem@upstash.com>",
  "license": "MIT",
  "main": "./dist/feedback-widget.umd.js",
  "module": "./dist/feedback-widget.es.js",
  "exports": {
    ".": {
      "import": "./dist/feedback-widget.es.js",
      "require": "./dist/feedback-widget.umd.js"
    },
    "./": "./"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm build-ui && pnpm build-api",
    "build-ui": "tsc && vite build --config vite-ui.config.ts",
    "build-api": "tsc && vite build --config vite-api.config.ts"
  },
  "dependencies": {
    "@upstash/redis": "^0.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "prettier": "^2.5.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "fluid": false
  },
  "homepage": "https://github.com/upstash/feedback#readme",
  "repository": {
    "type": "git",
    "url": "git+https://ademilter@github.com/upstash/feedback.git"
  },
  "bugs": {
    "url": "https://github.com/upstash/feedback/issues"
  }
}
